{
  "name": "tech-fvi-edu",
  "version": "1.0.0",
  "description": "Website for tech.fvi.edu",
  "main": "index.html",
  "scripts": {
    "build-js": "browserify js/main.js -o js/bundle.js",
    "build-min-js": "browserify js/main.js | uglifyjs -o js/bundle.min.js",
    "build-sass": "node-sass css/sass/main.scss css/main.css",
    "build-min-sass": "node-sass css/sass/main.scss | cleancss -o css/main.min.css",
    "watch-js": "nodemon -e js -w js -x 'npm run build-js'",
    "watch-sass": "nodemon -e scss -w sass -x 'npm run build-sass'",
    "build": "npm run build-js & npm run build-sass",
    "watch": "npm run watch-js & npm run watch-sass",
    "build-min": "npm run build-min-js & npm run build-min-sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FVITech/tech-fvi-edu.git"
  },
  "author": "Brenton Cozby -- bcozby@fvi.edu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FVITech/tech-fvi-edu/issues"
  },
  "homepage": "https://github.com/FVITech/tech-fvi-edu#readme",
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "dependecies": {
    "envify": "^3.4.1",
    "babelify": "^7.3.0"
  },
  "devDependencies": {
    "clean-css": "^3.4.20",
    "node-sass": "^3.10.1",
    "uglifyjs": "^2.4.10"
  }
}
